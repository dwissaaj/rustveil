{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "fs:default",
    {
      "identifier": "fs:allow-app-read-recursive",
      "allow": [
        {
          "path": "$APPDATA/*"
        }
      ]
    },
    "opener:default",
    "dialog:default",
    "fs:default",
    "store:default"
  ]
}
