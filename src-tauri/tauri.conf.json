{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Rustveil",
  "version": "0.1.0",
  "identifier": "com.hakuro.space",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Rustveil",
        "width": 1200,
        "height": 900,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },

  "bundle": {
    "resources": ["models/"],
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "longDescription": "An App for handle your Natural Language Processing with support of All LLM Model to better performance and accuracy",
    "shortDescription": "An App for handle Natural Language Processing Task",
    "publisher": "Hakuro Space"
  }
}
